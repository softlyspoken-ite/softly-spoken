<body>
    <div class="container-xl">
        <div id="login" class="row col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="col-lg-6 col-md-6">
                <img class="pic-login-2" src="http://localhost:3000/resources/pic-login-2.svg">
            </div>
            <div class="col-lg-5 col-md-6 col-sm-12 col-12 login-form">
                <h4>Pleased to meet you</h4>
                <h5>tell your story with SoftlySpoken.</h5>
                <div class="form-group">
                    <form action="users" method="post">
                        <input type="text" class="form-control form-control-lg" id="email" name="email"
                            placeholder="Email Address" required>
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-envelope-fill"
                            fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd"
                                d="M.05 3.555A2 2 0 0 1 2 2h12a2 2 0 0 1 1.95 1.555L8 8.414.05 3.555zM0 4.697v7.104l5.803-3.558L0 4.697zM6.761 8.83l-6.57 4.027A2 2 0 0 0 2 14h12a2 2 0 0 0 1.808-1.144l-6.57-4.027L8 9.586l-1.239-.757zm3.436-.586L16 11.801V4.697l-5.803 3.546z" />
                        </svg>
                        <input type="text" class="form-control form-control-lg" placeholder="Username" name="name"
                            required>
                        <svg width="1.2em" height="1.2em" viewBox="0 0 16 16" class="bi bi-person-fill"
                            fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd"
                                d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                        </svg>
                        <input type="password" class="form-control form-control-lg" placeholder="Password"
                            name="password" required>
                        <svg width="1.2em" height="1.2em" viewBox="0 0 16 16" class="bi bi-lock-fill"
                            fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path d="M2.5 9a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2h-7a2 2 0 0 1-2-2V9z" />
                            <path fill-rule="evenodd" d="M4.5 4a3.5 3.5 0 1 1 7 0v3h-1V4a2.5 2.5 0 0 0-5 0v3h-1V4z" />
                        </svg>
                        <input type="password" class="form-control form-control-lg" placeholder="Confirm Password"
                            required>
                        <svg width="1.2em" height="1.2em" viewBox="0 0 16 16" class="bi bi-lock-fill"
                            fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path d="M2.5 9a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2h-7a2 2 0 0 1-2-2V9z" />
                            <path fill-rule="evenodd" d="M4.5 4a3.5 3.5 0 1 1 7 0v3h-1V4a2.5 2.5 0 0 0-5 0v3h-1V4z" />
                        </svg>
                        <div class="login-btn">
                            <button class="btn btn-primary">Sign Up</button>
                        </div>
                    </form>
                </div>






            </div>
        </div>


    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
