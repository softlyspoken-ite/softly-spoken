<div class="music-player">
  <div class="progress-wrapper">
    <div class="current-song">Current</div>
    <input type="range" min="1" max="100" value="1" class="seek_slider" onchange="seekTo()">
    <div style="display: inline-flex;">
      <div class="start-time">0:00</div>
      <div class="end-time">0:00</div>
    </div>
  </div>
  <div class="player-actions" style="margin-left: 80px;">
    <button class="prev-button" onclick="backward10sec()">
      <img src="http://localhost:3000/resources/backward.png" style="width: 30px;height: 30px;">
    </button>
    <button class="play-button" onclick="playpauseTrack()">
      <img src="http://localhost:3000/resources/play.png"
        style="width: 28px;height: 33px;border-radius: 0%;margin-left: 5px;margin-top: 5px;">
    </button>
    <button class="next-button" onclick="forward10sec()">
      <img src="http://localhost:3000/resources/forward.png" style="width: 30px;height: 30px;">
    </button>
    <a href="#" class="track-art"></a>
    <div class="volume">
      <div class="volume-btn" onclick="show()">
        <button></button>
      </div>
      <div class="volume-controls">
        <input type="range" orient="vertical" min="1" max="100" value="100" class="volume_slider" onchange="setVolume()">
      </div>
    </div>
    <a href="#" class="profile">
      <img src="http://localhost:3000/resources/profile.png" class="profileimg">
      <a href="#" class="profilename track-artist">Untitle Case</a>
    </a>
    <a hef="#" class="saved">
    </a>
  </div>
</div>